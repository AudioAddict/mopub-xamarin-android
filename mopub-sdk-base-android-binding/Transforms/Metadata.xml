<metadata>
  <attr path="/api/package[@name='com.mopub.common']" name="managedName">MoPub.Common</attr>
  <attr path="/api/package[@name='com.mopub.common.event']" name="managedName">MoPub.Common.Events</attr>
  <attr path="/api/package[@name='com.mopub.exceptions']" name="managedName">MoPub.Exceptions</attr>
  <attr path="/api/package[@name='com.mopub.mobileads']" name="managedName">MoPub.MobileAds</attr>
  <attr path="/api/package[@name='com.mopub.volley']" name="managedName">MoPub.Volley</attr>

  <attr path="/api/package[@name='com.mopub.mobileads']/class[@name='BaseInterstitialActivity']" name="visibility">public</attr>
  <!--  <attr path="/api/package[@name='com.mopub.mobileads']/interface[@name='CustomEventInterstitialAdapter.CustomEventInterstitialAdapterListener']" name="visibility">public</attr>
  <attr path="/api/package[@name='com.mopub.mobileads']/interface[@name='CustomEventRewardedVideo.CustomEventRewardedVideoListener']" name="visibility">public</attr> -->

  <!--add node to make AsyncTask works
  https://stackoverflow.com/questions/43838615/xamarin-binding-jar-gives-error-does-not-implement-inherited-abstract-member-as -->
  <add-node path="/api/package[@name='com.mopub.common']">
    <class abstract="false" deprecated="not deprecated" final="false" name="UrlResolutionTask" static="true" visibility="public" extends="java.lang.Object"/>
  </add-node>
  <add-node path="/api/package[@name='com.mopub.mobileads']">
    <class abstract="false" deprecated="not deprecated" final="false" name="VastVideoBlurLastVideoFrameTask" static="true" visibility="public" extends="java.lang.Object"/>
    <class abstract="false" deprecated="not deprecated" final="false" name="VastXmlManagerAggregator" static="true" visibility="public" extends="java.lang.Object"/>
  </add-node>

  <!--resolve IComparable issue
  https://bugzilla.xamarin.com/show_bug.cgi?id=8112#c1 -->
  <add-node path="/api/package/class[implements[@name='java.lang.Comparable']]">
    <method name="compareTo" return="int" abstract="false" native="false" synchronized="false" static="false" final="false" deprecated="not deprecated" visibility="public">
      <parameter name="o" type="java.lang.Object" />
    </method>
  </add-node>
  <add-node path="/api/package/class[implements[@name='java.util.Comparator']]">
    <method name="compare" return="int" abstract="false" native="false" synchronized="false" static="false" final="false" deprecated="not deprecated" visibility="public">
      <parameter name="o1" type="java.lang.Object" />
      <parameter name="o2" type="java.lang.Object" />
    </method>
  </add-node>

  <attr path="/api/package[@name='com.mopub.common.event']/class[@name='ErrorEvent.Builder']/method[@name='build']" name="final">true</attr>
  <attr path="/api/package[@name='com.mopub.common.event']/class[@name='ErrorEvent.Builder']/method[@name='build']" name="visibility">private</attr>
  <attr path="/api/package[@name='com.mopub.common.event']/class[@name='ErrorEvent.Builder']/method[@name='build']" name="managedName">PrivateBuild</attr>

  <attr path="/api/package[@name='com.mopub.common.event']/class[@name='Event.Builder']/method[@name='build']" name="final">true</attr>
  <attr path="/api/package[@name='com.mopub.common.event']/class[@name='Event.Builder']/method[@name='build']" name="visibility">private</attr>
  <attr path="/api/package[@name='com.mopub.common.event']/class[@name='Event.Builder']/method[@name='build']" name="managedName">PrivateBuild</attr>

  <remove-node path="/api/package[@name='com.mopub.mobileads']/class[@name='VastCompanionAdConfig']" />

</metadata>
